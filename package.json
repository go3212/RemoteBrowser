{
  "name": "remote-browser-orchestrator",
  "version": "1.0.0",
  "description": "Orchestrator for ephemeral browserless/chrome containers with session persistence",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "test": "tsx test/run-comprehensive.ts",
    "test:quick": "tsx test/quick-test.ts",
    "test:all": "npm run test:health && npm run test:error && npm run test:session && npm run test:browser && npm run test:interaction && npm run test:profiles && npm run test:advanced",
    "test:comprehensive": "tsx test/run-comprehensive.ts",
    "test:health": "tsx test/health.test.ts",
    "test:session": "tsx test/session.test.ts",
    "test:browser": "tsx test/browser.test.ts",
    "test:interaction": "tsx test/interaction.test.ts",
    "test:error": "tsx test/error-handling.test.ts",
    "test:profiles": "tsx test/profiles.test.ts",
    "test:advanced": "tsx test/advanced.test.ts",
    "build:worker": "docker build -t remote-browser-worker src/worker"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "cors": "^2.8.5",
    "dockerode": "^4.0.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "multer": "^1.4.5-lts.1",
    "playwright": "1.41.2",
    "tar-fs": "^3.0.4",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.23",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/tar-fs": "^2.0.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "axios": "^1.13.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
